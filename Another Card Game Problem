/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
   
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		while(t>0){
		    int pc=sc.nextInt();
		    int pr=sc.nextInt();
		    int pc_digit=(int)Math.ceil((float)pc/9);
		    int pr_digit=(int)Math.ceil((float)pr/9);
		   
		    if(pc_digit==pr_digit){
		        System.out.println(1+" "+pr_digit);
		    }else if(pc_digit<pr_digit){
		        System.out.println(0+" "+pc_digit);
		    }else{
		        System.out.println(1+" "+pr_digit);
		    }
		    t--;
		}
	}
}
